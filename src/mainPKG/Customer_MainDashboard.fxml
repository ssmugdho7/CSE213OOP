<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: gray;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainPKG.Customer_MainDashboardController">
   <children>
      <VBox layoutX="8.0" layoutY="7.0" prefHeight="380.0" prefWidth="251.0" style="-fx-background-color: white;">
         <children>
            <Label text="Customer Dashboard" textFill="#238262">
               <font>
                  <Font name="Century Gothic Bold" size="23.0" />
               </font>
            </Label>
            <Button fx:id="paybill" onAction="#handlePayBillButtonClick" prefHeight="31.0" prefWidth="264.0" text="Pay Bill" textFill="RED" />
            <Button fx:id="paymenthistory" onAction="#handlePaymentHistoryButtonClick" prefHeight="31.0" prefWidth="263.0" text="Payment History" textFill="#3011ff" />
            <Button fx:id="viewandmodiservice" onAction="#handleServicePlanButtonClick" prefHeight="31.0" prefWidth="263.0" text="View and Modify Service Plan" textFill="RED" />
            <Button fx:id="ompissues" onAction="#handleComplaintButtonClick" prefHeight="31.0" prefWidth="262.0" text="Complaint Issues" textFill="RED" />
            <Button fx:id="sessionlogreopo" onAction="#handleSessionLogButtonClick" prefHeight="31.0" prefWidth="256.0" text="Session Log Reports" textFill="#3011ff" />
            <Button fx:id="acessserver" onAction="#handleAccessServersButtonClick" prefHeight="31.0" prefWidth="258.0" text="Access Servers" textFill="#3011ff" />
            <Button fx:id="checkwifispeed" onAction="#handleCheckSpeedButtonClick" prefHeight="31.0" prefWidth="257.0" text="Check WiFi Speed" textFill="#3011ff" />
            <Button fx:id="feedback" onAction="#handleFeedbackButtonClick" prefHeight="31.0" prefWidth="256.0" text="Feedback and Automated Replies" textFill="RED" />
            <Button fx:id="logOut" onAction="#handleLogoutButtonClick" prefHeight="31.0" prefWidth="257.0" style="-fx-background-color: red;" text="Logout" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
